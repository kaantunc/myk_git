.up_submitbtn    {
	background-image: url(images/button.gif);
	border: 1px solid #000033;
	width: 82px;
	font-family: Geneva, Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #FFFFFF;
	font-weight: bold;
	background-position: center;
	padding: 0px;
	margin-bottom: 0px;
	margin-left: 10px;
}
.up_silbtn    {
	background-image: url(images/buttonred.gif);
	background-repeat: repeat-x;
	border: 1px solid #000033;
	font-family: Geneva, Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #FFFFFF;
	font-weight: bold;
	background-position: center;
	padding: 0px;
	margin-right: 10px;
	margin-bottom: 0px;
	margin-left: 6px;
}

.up_success{
	background-image: url(images/ok.gif);
	background-position: left;
	padding-left: 25px;
	padding-top: 4px;
	background-repeat: no-repeat;
	font-weight: bold;
	color: #449944;
	height: 24px;
}

.up_error{
	background-image: url(images/nok.gif);
	background-position: left;
	padding-left: 25px;
	padding-top: 4px;
	background-repeat: no-repeat;
	font-weight: bold;
	color: #994444;
	height: 24px;
}